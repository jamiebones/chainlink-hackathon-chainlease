# ChainLease Credit Check Workflow Environment Variables
# Copy this file to .env and fill in your actual values

# ========================================
# EVM Configuration
# ========================================
# Private key for signing transactions (without 0x prefix)
SIGNER_PRIVATE_KEY=your_private_key_here

# Deployed LeaseAgreement contract address
LEASE_AGREEMENT_ADDRESS=0x0000000000000000000000000000000000000000

# ========================================
# Credit Check API
# ========================================
# External credit bureau API endpoint
CREDIT_API_ENDPOINT=https://api.creditcheck.example.com/v1/verify

# API key for credit check service
CREDIT_API_KEY=your_api_key_here

# ========================================
# Backend API (for database operations)
# ========================================
# Backend API endpoint for saving credit check data
BACKEND_API_ENDPOINT=http://localhost:3000/api/data/credit-checks

# ========================================
# Chain Configuration
# ========================================
# Available options:
# - ethereum-testnet-sepolia (for testing)
# - ethereum-mainnet (for production)
CHAIN_SELECTOR_NAME=ethereum-testnet-sepolia
